                        "docking-experimental"
                    ]
                },
                {
                    "name": "fastgltf",
                    "version": "0.9.0"
                },
                {
                    "name": "spirv-tools",
                    "version": "1.4.309.0"
                },
                {
                    "name": "shader-slang",
                    "version": "2025.14.3"
                },
                {
                    "name": "glaze",
                    "version": "6.0.1"
                }
            ]
        }
