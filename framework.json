                {
                    "name": "portal-core",
                    "version": "0.1.0"
                },
                {
                    "name": "portal-application",
                    "version": "0.1.0"
                },
                {
                    "name": "glfw3",
                    "version": "3.4#1"
                },
                {
                    "name": "glaze",
                    "version": "6.0.1"
                }
            ]
        },
        {
            "name": "portal-networking",
            "short_name": "networking",
            "version": "0.1.0",
            "description": "The networking interface for the Portal framework",
            "dependencies": [
                {
                    "name": "portal-core",
                    "version": "0.1.0"
                },
                {
                    "name": "gamenetworkingsockets",
                    "version": "1.4.1#1"
                }
            ]
        },
        {
            "name": "portal-serialization",
            "short_name": "serialization",
            "version": "0.1.0",
            "description": "The serialization interface for the Portal framework",
            "dependencies": [
                {
                    "name": "portal-core",
                    "version": "0.1.0"
                },
                {
                    "name": "nlohmann-json",
                    "version": "3.11.3#1"
                }
            ]
        },
        {
            "name": "portal-engine",
            "short_name": "engine",
            "version": "0.1.0",
            "description": "A game engine built on top of the Portal framework",
            "dependencies": [
                {
                    "name": "portal-input",
                    "version": "0.1.0"
                },
                {
                    "name": "portal-networking",
                    "version": "0.1.0"
                },
                {
                    "name": "portal-serialization",
                    "version": "0.1.0"
                },
                {
                    "name": "portal-serialization",
                    "version": "0.1.0"
                },
                {
                    "name": "concurrentqueue",
                    "version": "1.0.4"
                },
                {
                    "name": "vulkan-memory-allocator",
                    "version": "3.1.0"
