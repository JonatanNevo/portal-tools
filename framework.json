{
    "repo": "JonatanNevo/portal-framework",
    "repo_branch": "fix/vcpkg-packaging",
    "vcpkg_registry_repo": "JonatanNevo/portal-vcpkg-registry",
    "modules": [
        {
            "name": "llvm-adt",
            "short_name": "llvm-adt",
            "target_override": "llvm::adt",
            "version": "0.1.0",
            "description": "The ADT containers from the llvm project"
        },
        {
            "name": "portal-core",
            "short_name": "core",
            "version": "0.1.0",
            "description": "Application interface for the Portal framework",
            "options": [
                "PORTAL_FIND_PACAKGE=ON"
            ],
            "dependencies": [
                {
                    "name": "llvm-adt",
                    "version": "0.1.0"
                },
                {
                    "name": "glm",
                    "version": "1.0.2"
                },
                {
                    "name": "spdlog",
                    "version": "1.16.0"
                },
                {
                    "name": "enchantum",
                    "version": "0.3.0"
                },
                {
                    "name": "mimalloc",
                    "version": "2.2.3#1",
                    "features": [
                        "override"
                    ]
                }
            ]
        },
        {
            "name": "portal-application",
            "short_name": "application",
            "version": "0.1.0",
            "description": "The application interface for the Portal framework",
            "dependencies": [
                {
                    "name": "portal-core",
                    "version": "0.1.0"
                }
            ]
        },
        {
            "name": "portal-input",
            "short_name": "input",
            "version": "0.1.0",
